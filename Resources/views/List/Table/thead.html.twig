<thead>
  <tr>
    {% for col in section.headerColumns %}
      <th class="{{ col.fieldname }}-field {{ col.size }} {{ col.align }} {{ col.options.class is defined ? col.options.class : "" }}" {{ col.attr ? : "" }}>{{ col.entitled|trans({}, section.translateDomain )|raw() }}</th>
    {% endfor %}

    {% if section.columnActionsDefined %}
      <th class="actions-object"></th>
    {% endif %}
  </tr>
</thead>